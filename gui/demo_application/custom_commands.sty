\newcommand{\absstar}[2][]{%
  \def\tempa{}%
  \def\tempb{#1}%
  \ifx\tempa\tempb%
    \lvert#2\rvert%
  \else%
    \csname #1l\endcsname\lvert#2\csname #1r\endcsname\rvert%
  \fi%
}

\newcommand{\normstar}[2][]{%
  \def\tempa{}%
  \def\tempb{#1}%
  \ifx\tempa\tempb%
    \lVert#2\rVert%
  \else%
    \csname #1l\endcsname\lVert#2\csname #1r\endcsname\rVert%
  \fi%
}

\newcommand{\scalarstar}[2][]{%
  \def\tempa{}%
  \def\tempb{#1}%
  \ifx\tempa\tempb%
    \langle#2\rangle%
  \else%
    \csname #1l\endcsname\langle#2\csname #1r\endcsname\rangle%
  \fi%
}

\newcommand{\absnoStar}[1]{\left\lvert#1\right\rvert}
\newcommand{\normnoStar}[1]{\left\lVert#1\right\rVert}
\newcommand{\scalarnoStar}[1]{\left\langle#1\right\rangle}
